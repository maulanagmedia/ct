<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true"
        android:state_pressed="true">

        <shape android:shape="oval">
            <stroke android:color="@color/color_white" android:width="@dimen/line_size1"></stroke>
            <solid android:color="@color/color_white_trasnparent"></solid>
        </shape>
    </item>
    <item android:state_enabled="true"
        android:state_focused="true">

        <shape android:shape="oval">
            <stroke android:color="@color/color_white" android:width="@dimen/line_size1"></stroke>
            <solid android:color="@color/color_white_trasnparent"></solid>
        </shape>
    </item>
    <item android:state_enabled="true"
        android:state_selected="true">

        <shape android:shape="oval">
            <stroke android:color="@color/color_white" android:width="@dimen/line_size1"></stroke>
            <solid android:color="@color/color_white_trasnparent"></solid>
        </shape>
    </item>
    <item>

        <shape android:shape="oval">
            <stroke android:color="@color/color_white" android:width="@dimen/line_size1"></stroke>
            <solid android:color="@color/color_trasnparent"></solid>
        </shape>
    </item>

</selector>