<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainCoverage.MainCoverage"
    android:background="@color/color_white">

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/header"
        android:id="@+id/v_header"/>

    <id.net.gmedia.gmedialiveconnection.CustomView.CustomMapView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/v_header"
        android:id="@+id/mv_map"></id.net.gmedia.gmedialiveconnection.CustomView.CustomMapView>

</RelativeLayout>